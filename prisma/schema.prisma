generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String    @id @default(uuid())
  name     String    @unique
  password String
  role     RoleUsers @default(USER)
}

model test {
  id   Int    @id
  name String
}

enum RoleUsers {
  ADMIN
  USER
}
